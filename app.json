{
  "expo": {
    "name": "delivery",
    "slug": "delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.green.delivery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow Green to access your camera",
          "photosPermission": "Allow Green access to your photos"
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoibGF6eWRldjMzIiwiYSI6ImNsaDlwMjI1YTA5cnMzbGxwOWR0NTRmZGMifQ.IFaNaoaHmToqPxuWscJg1A"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "00e5ba92-f02a-4215-9005-8188ef9a6a92"
      }
    }
  }
}
